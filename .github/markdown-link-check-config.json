{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^http://node1-ip"
    },
    {
      "pattern": "^http://node2-ip"
    },
    {
      "pattern": "^http://load-balancer-ip"
    },
    {
      "pattern": "^http://your-server"
    },
    {
      "pattern": "computer://"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "application/vnd.github+json"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 301, 302, 307, 308]
}
